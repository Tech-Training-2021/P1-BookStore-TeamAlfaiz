@model  dynamic
@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>
<h2>@ViewBag.v</h2>
<table class="table">
    <thead>
        <tr>
            <th>Customer Id</th>
            <th>Customer Name</th>
            <th>Customer Email</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var cust in Model)
        {
        <tr>
            <th scope="row">@cust.Id</th>
            <td>@cust.Name</td>
            <td>@cust.Email</td>
            <td>@Html.ActionLink("Details", "GetCustomerById", new { id = cust.Id }) |</td>
        </tr>
        }
    </tbody>
</table>

